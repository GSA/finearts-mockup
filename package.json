{
  "name": "finearts-mockup",
  "description": "Mockup for the Fine Arts website",
  "scripts": {
    "start": "concurrently npm:uswds:watch npm:serve",
    "serve": "eleventy --serve",
    "clean": "rm -r _site",
    "build": "eleventy",
    "build:clean": "npm run clean && npm run build",
    "uswds:watch": "gulp watch",
    "uswds:compile": "gulp compile"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@uswds/compile": "^1.0.0-beta.3",
    "@uswds/uswds": "^3.0.2",
    "concurrently": "^7.2.2",
    "gulp": "^4.0.2"
  }
}
